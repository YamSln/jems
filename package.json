{
  "name": "jems",
  "version": "1.1.0",
  "description": "jems game",
  "main": "dist/index",
  "typings": "dist/index",
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/client/"
    ]
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "debug": "tsc && node --inspect dist/index.js",
    "start": "node --optimize_for_size --max_old_space_size=460 dist/index.js",
    "build": "npm install && tsc && cd src/client && npm install && npm run build",
    "test": "ts-node -O '{\"config\":\"commonjs\"}' node_modules/jest/bin/jest.js"
  },
  "author": "Jam",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "socket.io": "^4.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.180",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.6"
  },
  "engines": {
    "node": "16.x",
    "npm": "6.x"
  }
}
